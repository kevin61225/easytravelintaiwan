@model List<EasyTravelInTaiwan.Models.view>
<style>
    /*#FavorWrapper .box {
        height: auto;
    }*/
</style>
<div id="favorSuggestPlace" class="well" style="max-height: 175px">
    <ul id="FavorWrapper" class="nav nav-pills nav-stacked">
        @foreach (EasyTravelInTaiwan.Models.view place in Model)
        {
            <li id="place.@place.Id"><a href="#">@place.Name</a></li>
        }
    </ul>
</div>
<script type="text/javascript">
    $(document).ready(function () {
        $("#favorSuggestPlace").niceScroll({ autohidemode: true });
    });
   
    $('#FavorWrapper li').click(function () {
        var str = $(this).attr('id');
        var n = str.split(".");
        var id;
        for (var i = 0; i < gmarkers.length; i++) {
            if (gmarkers[i].gId == n[1]) {
                id = i;
                console.log(id);
            }
        }
        SetGmarkerVisible(id);
        //google.maps.event.trigger(gmarkers[id], "click");
        //gmarkers[id].setVisible(true);
    });
</script>
